{
  "name": "interviewExercise",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "ios": "npx react-native run-ios",
    "ios:small": "npx react-native run-ios --simulator='iPhone SE (2nd generation)'",
    "ios:device": "npx react-native run-ios --device",
    "start": "npx react-native start",
    "start:reset": "npx react-native start --reset-cache",
    "reset:watchman": "watchman watch-del-all && watchman shutdown-server",
    "nuke:ios": "rm -rf node_modules && rm -rf ios/Pods && yarn && cd ios && bundle exec pod install",
    "pods": "cd ios && bundle exec pod install && cd ..",
    "test": "jest"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.5.14",
    "@react-navigation/native": "^6.1.12",
    "@react-navigation/native-stack": "^6.9.20",
    "@reduxjs/toolkit": "^2.2.1",
    "@shopify/flash-list": "^1.6.3",
    "axios": "^1.6.7",
    "react": "18.2.0",
    "react-native": "0.73.5",
    "react-native-gesture-handler": "^2.15.0",
    "react-native-mask-input": "^1.2.3",
    "react-native-reanimated": "^3.7.1",
    "react-native-reanimated-carousel": "^3.5.1",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-screens": "^3.29.0",
    "react-redux": "^9.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
